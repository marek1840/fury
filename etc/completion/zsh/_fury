#compdef fury

__fury_completions(){
    offset=$1
    words=$2
    fury complete --shell zsh --offset $offset -- $words 2> /dev/null ||
        echo "1:Command:((start:'start the Fury daemon'))"
}

_fury() {
  oldIfs=$IFS
  IFS=$'\n'
  results=($(__fury_completions $CURRENT $words))
  results=($results '*:dummy:')
  IFS=$oldIfs
  _arguments -C $results
}

_fury

return 0
